{
  "name": "electron-vue",
  "productName": "electron-vue",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "start:needle": "vite --host",
    "dev": "node vite-dev.js",
    "dashboard": "node serve-dashboard.js",
    "electron": "./start-electron.sh",
    "dev:electron": "node start-dev.js",
    "dev:esm": "node build-config/dev-runner.js",
    "build": "node build-config/build-runner.js",
    "build:vue": "vite build --config build-config/vite.vue.config.mjs",
    "build:svelte": "vite build --config build-config/vite.svelte.config.mjs",
    "build:threejs": "vite build --config build-config/vite.threejs.config.mjs",
    "build:main": "vite build --config build-config/vite.main.config.mjs",
    "build:main-renderer": "vite build --config build-config/vite.main-renderer.config.mjs",
    "build:dev": "vite build",
    "build:production": "vite build -- --production",
    "gltf:transform": "npm run transform --prefix node_modules/@needle-tools/gltf-build-pipeline",
    "start": "electron-forge start",
    "start:dev": "npm run dev",
    "package": "npm run build && electron-forge package",
    "make": "npm run build && electron-forge make",
    "publish": "npm run build && electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
